lay'>\n              <input\n                type='text'\n                value={newName}\n                onChange={(event) => handleChange(event)}\n                disabled={false}\n              />\n              <input type='text' value={props.selected.email} disabled={true} />\n              <input\n                type='text'\n                value={props.selected.response}\n                disabled={true}\n              />\n              <input\n                type='text'\n                value={props.selected.visited}\n                disabled={true}\n              />\n            </div>\n            <Button\n              text='Save'\n              clickAction={() => editInvite(props.selected.name, newName)}\n              className='editInvite'\n            />\n            <Button\n              text='Back to List'\n              clickAction={() => props.toggleList()}\n              className='backButton'\n            />\n            <Button\n              text='Back'\n              clickAction={() => setEditForm(false)}\n              className='removeInvite'\n            />\n          </>\n        ) : (\n          <>\n            <div className='infoDisplay'>\n              <input type='text' value={props.selected.name} disabled={true} />\n              <input type='text' value={props.selected.email} disabled={true} />\n              <input\n                type='text'\n                value={props.selected.response}\n                disabled={true}\n              />\n              <input\n                type='text'\n                value={props.selected.visited}\n                disabled={true}\n              />\n            </div>\n            <Button\n              text='Edit Invite'\n              clickAction={() => setEditForm(true)}\n              className='editInvite'\n            />\n            <Button\n              text='Back to List'\n              clickAction={() => props.toggleList()}\n              className='backButton'\n            />