an>\n              <strong>{this.addLeadingZeros(countDown.days)}</strong>\n              <span>{countDown.days === 1 ? 'Day' : 'Days'}</span>\n        </span>\n\n        <span>\n            <strong>{this.addLeadingZeros(countDown.hours)}</strong>\n            <span>Hours</span>\n        </span>\n\n\n        <span>\n            <strong>{this.addLeadingZeros(countDown.min)}</strong>\n            <span>Min</span>\n        </span>\n\n        <span>\n            <strong>{this.addLeadingZeros(countDown.sec)}</strong>\n            <span>Sec</span>\n        </span>\n      </div>\n    );\n  }\n}\n\n\nexport default Countdown;","import React from 'react';\n\nclass Menu extends React.Component {\n\n    render() {\n        return (<div className=\"options\">\n                    <p onClick={() => this.props.menuSelection('home')}>Home</p>\n                    <p onClick={() => this.props.menuSelection('wedding')}>Wedding</p>\n                    <p onClick={() => this.props.menuSelection('travel')}>Travel</p>\n                    <p onClick={() => this.props.menuSelection('rsvp')}>RSVP</p>\n                </div>);\n    }\n}\n\nexport default Menu;\n\n","function SignInButton(props) {\n    if(props.openForm){\n        return null;\n    } else {\n        return (\n            <p className=\"signedInButton\" onClick={props.toggleForm}>SignIn</p>\n        )\n    }    \n}\n\nexport default SignInButton;","import Countdown from \"../components/Header/Countdown\";\nimport Menu from '../components/Header/Menu';\nimport React from \"react\";\nimport SignInButton from '../components/Header/SignInButton';\n\nclass Header extends React.Component {\n    render() {\n        if(this.props.signedIn){\n            return (\n                <div className=\"headerSignedIn\">\n                    <Countdown />\n                    <div className=\"message\">\n                        <p>Paul and Elyse</p>\n                        <p>Wedding</p>\n                    </div>      \n                