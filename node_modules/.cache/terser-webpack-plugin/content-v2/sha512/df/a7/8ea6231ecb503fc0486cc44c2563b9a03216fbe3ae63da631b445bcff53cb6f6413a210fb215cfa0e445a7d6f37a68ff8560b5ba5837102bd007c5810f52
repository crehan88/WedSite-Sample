     sec: 0,\n    }\n  }\n\n  componentDidMount() {\n    // update every second\n    this.interval = setInterval(() => {\n      const date = this.calculateCountdown(this.props.date);\n      date ? this.setState(date) : this.stop();\n    }, 1000);\n  }\n\n  componentWillUnmount() {\n    this.stop();\n  }\n\n  calculateCountdown() {\n    let diff = (Date.parse(new Date('August 4, 2022 19:00:00')) - Date.parse(new Date())) / 1000;\n\n    // clear countdown when date is reached\n    if (diff <= 0) return false;\n\n    const timeLeft = {\n      days: 0,\n      hours: 0,\n      min: 0,\n      sec: 0\n    };\n\n    if (diff >= 86400) { // 24 * 60 * 60\n      timeLeft.days = Math.floor(diff / 86400);\n      diff -= timeLeft.days * 86400;\n    }\n    if (diff >= 3600) { // 60 * 60\n      timeLeft.hours = Math.floor(diff / 3600);\n      diff -= timeLeft.hours * 3600;\n    }\n    if (diff >= 60) {\n      timeLeft.min = Math.floor(diff / 60);\n      diff -= timeLeft.min * 60;\n    }\n    timeLeft.sec = diff;\n\n    return timeLeft;\n  }\n\n  stop() {\n    clearInterval(this.interval);\n  }\n\n  addLeadingZeros(value) {\n    value = String(value);\n    while (value.length < 2) {\n      value = '0' + value;\n    }\n    return value;\n  }\n\n  render() {\n    const countDown = this.state;\n\n    return (\n      <div className=\"countdown\">\n        <span>\n          <strong>{this.addLeadingZeros(countDown.days)}</strong>\n          <span>{countDown.days === 1 ? 'Day' : 'Days'}</span>\n        </span>\n\n        <span>\n          <strong>{this.addLeadingZeros(countDown.hours)}</strong>\n          <span>Hours</span>\n        </span>\n\n\n        <span>\n          <strong>{this.addLeadingZeros(countDown.min)}</strong>\n          <span>Min</span>\n        </span>\n\n        <span>\n          <strong>{this.addLeadingZeros(countDown.sec)}</strong>\n          <span>Sec</span>\n        </span>\n      </div>\n    );\n  }\n}\n\n\nexport default Countdown;\n","import axios from 'axios';\nimport { API } from '../Constants';\n\nexport function CheckAuth(name, password) {\n  return axios({\n    method: 'POST',\n    baseURL: API + 'signin',\n    data: {\n      name: name,\n      password: password\n    }\n  });\n}\n\nexport function Rsvp(rsvp, auth) {\n  return axios({\n    method: 'POST',\n    baseURL: API + 'rsvp',\n    headers: {\n      authorization: auth\n    },\n    data: {\n      response: rsvp.response,\n      text: rsvp.text,\n      email: rsvp.email\n    }\n  });\n}\n\nexport function GetRsvp(auth) {\n  return axios({\n    method: 'POST',\n    baseURL: API + 'getrsvp',\n    headers: {\n      authorization: auth\n    }\n  });\n}\nexport function GetMessages() {\n\n}\n","// extracted by mini-css-extract-plugin","// extracted by mini-css-extract-plugin","import { useState } from 'react';\nimport { CheckAuth } from '../../Data/Database';\nimport { Formik, Field, Form } from 'formik';\n\nexport default function SignInForm(props) {\n\n  const [nameError, setNameError] = useState(false);\n  const [serverError, setServerError] = useState(false);\n\n  return (\n    <Formik\n      initialValues={{\n        name: '',\n        password: ''\n      }}\n      onSubmit={(values, actions) => {\n        CheckAuth(values.name, values.password)\n          .then((data) => {\n            if (data.status === 203) {\n              setNameError(true);\n              actions.setSubmitting(false);\n            } else {\n              props.setAuth(data.headers.authorization);\n              props.setResponse(data.data);\n              actions.setSubmitting(false);\n            }\n          }).catch(() => {\n            setServerError(true);\n            actions.setSubmitting(false);\n          })\n      }}\n    >\n      {({ values, isSubmitting }) => (\n        <Form>\n          {nameError ? <p>\n            \"{values.name}\" does not match list of invites, Please try again</p>\n            : null}\n          {serverError ? <p>\n            Server may be down!! Please try again later.</p>\n            : null}\n          <Field type=\"text\" name=\"name\"\n            className=\"name\" placeholder=\"name\"\n          />\n          {values.name === 'admin' ?\n            <Field type=\"password\" name=\"password\"\n              className=\"password\" placeholder=\"password\" />\n            : null}\n          <button className=\"signInButton\"\n            type=\"submit\"\n            disabled={isSubmitting}>Join</button>\n        </Form>\n      )}\n    </Formik>\n  );\n}\n","import Countdown from \"../Layout/Common/Countdown\";\nimport SignInForm from '../Layout/SignIn/SignInForm';\nimport React, { useState } from 'react';\n\n\nexport function SignedOutPage(props) {\n\n    const [openForm, setOpenForm] = useState(false);\n\n    return (\n        <div className=\"signedOut\" >\n            <div className=\"container\">\n                <Countdown />\n                <div className=\"message\">\n                    <p>Paul</p><p>&</p><p>Elyse</p>\n                </div>\n                {openForm ?\n                    <SignInForm\n                        setResponse={props.setResponse}\n                        setAuth={props.setAuth} /> :\n                    <p className=\"signInButton\"\n                        onClick={setOpenForm(true)}>\n                        Join Us\n                    </p>}\n                <p className=\"contact\">Having trouble signining in? Contact Us at <i>paulandelysewedding@gmail.com</i></p>\n                <p className=\"photocredit\">Photo Credit: Brittany Fleming - Facebook: @bflemingphoto Instagram: @bflemingphotography</p>\n            </div>\n        </div >\n    );\n}\n","import './Styles/App.scss';\nimport React, { useState, useEffect, Suspense, lazy } from 'react';\nimport { SignedOutPage } from './Pages/SignedOutPage';\nimport Loader from \"react-loader-spinner\";\nconst SignedInPage = lazy(() => import('./Pages/SignedInPage'));\n\nfunction App() {\n\n  const [signedIn, setSignedIn] = useState(false);\n  const [auth, setAuth] = useState('');\n  const [response, setResponse] = useState('');\n\n  useEffect(() => {\n    if (auth !== '')\n      setSignedIn(true);\n  }, [auth]);\n\n  if (!signedIn) {\n    return <SignedOutPage\n      setAuth={setAuth}\n      setResponse={setResponse}\n    />\n  } else {\n    return (\n      <Suspense fallback={<div className=\"loader\">\n        <Loader type=\"TailSpin\" color=\"#00BFFF\" height={80} width={80} />\n      </div>\n      }>\n        <SignedInPage\n          auth={auth}\n          response={response}\n          setResponse={setResponse}\n        />\n      </Suspense>\n    )\n  }\n\n}\n\n\nexport default App;\n","const reportWebVitals = onPerfEntry => {\n  if (onPerfEntry && onPerfEntry instanceof Function) {\n    import('web-vitals').then(({ getCLS, getFID, getFCP, getLCP, getTTFB }) => {\n      getCLS(onPerfEntry);\n      getFID(onPerfEntry);\n      getFCP(onPerfEntry);\n      getLCP(onPerfEntry);\n      getTTFB(onPerfEntry);\n    });\n  }\n};\n\nexport default reportWebVitals;\n","import './Styles/index.scss';\n\nimport App from './App';\nimport React from 'react';\nimport ReactDOM from 'react-dom';\nimport reportWebVitals from './reportWebVitals';\n\nReactDOM.render(\n  <React.StrictMode>\n    <App />\n  </React.StrictMode>,\n  document.getElementById('root')\n);\n\n// If you want to start measuring performance in your app, pass a function\n// to log results (for example: reportWebVitals(console.log))\n// or send to an analytics endpoint. Learn more: https://bit.ly/CRA-vitals\nreportWebVitals();\n","export const PAGES = {\n    WEDDING: \"wedding\",\n    TRAVEL: \"travel\",\n    RSVP: \"rsvp\",\n    CONTACT: \"contact\",\n};\n\nexport const API = 'https://75d6250359.execute-api.us-east-1.amazonaws.com/'\n"]}}